		IF F_LENDING = 1 THEN DO ; 
		  	SP_LENDING = "Yes";
		    SP_LENDING_CODE = 'SPLE1';
		END;
		ELSE IF F_LENDING = 0 THEN DO;
			SP_LENDING = "No";
			SP_LENDING_CODE = 'SPLE2';
		END;
